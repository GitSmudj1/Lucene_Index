  <text>
    <textblock>Defense Contracting; Reporting Procedures on Defense-Related Employment</textblock>
  </text>
  <usdept>
    <textblock>DEPARTMENT OF DEFENSE</textblock>
  </usdept>
  <usbureau>
    <textblock>Office of the Secretary</textblock>
